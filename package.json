{
  "name": "ai-sdk-knowledge-base",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "next dev",
    "build": "next build",
    "start": "next start",
    "lint": "next lint",
    "format": "prettier --ignore-unknown --write .",
    "ts": "pnpm tsc --noEmit",
    "db:generate": "dotenv -c -- drizzle-kit generate --config=drizzle-dev.config.ts",
    "db:migrate": "dotenv -c -- drizzle-kit migrate  --config=drizzle.config.ts",
    "db:push": "dotenv -c -- drizzle-kit push --config=drizzle-dev.config.ts",
    "db:studio": "dotenv -c -- drizzle-kit studio --config=drizzle-dev.config.ts",
    "update-deps": "pnpm update --interactive --latest"
  },
  "dependencies": {
    "@ai-sdk/openai": "0.0.68",
    "@ai-sdk/provider": "0.0.24",
    "@langchain/textsplitters": "0.1.0",
    "@vercel/blob": "0.25.1",
    "@vercel/postgres": "0.10.0",
    "ai": "3.4.17",
    "bcrypt-ts": "5.0.2",
    "classnames": "2.5.1",
    "drizzle-orm": "0.35.2",
    "framer-motion": "12.0.0-alpha.1",
    "next": "15.0.0-canary.205",
    "next-auth": "5.0.0-beta.25",
    "pdf-parse": "1.1.1",
    "postgres": "3.4.4",
    "react": "19.0.0-rc-65a56d0e-20241020",
    "react-dom": "19.0.0-rc-65a56d0e-20241020",
    "react-markdown": "9.0.1",
    "remark-gfm": "4.0.0",
    "sonner": "1.5.0",
    "swr": "2.2.6-beta.4",
    "usehooks-ts": "3.1.0",
    "uuid": "11.0.0-0",
    "zod": "3.23.8"
  },
  "devDependencies": {
    "@types/node": "^20",
    "@types/pdf-parse": "1.1.4",
    "@types/react": "npm:types-react@rc",
    "@types/react-dom": "npm:types-react-dom@rc",
    "dotenv-cli": "7.4.2",
    "drizzle-kit": "0.26.2",
    "eslint": "8.57.0",
    "eslint-config-next": "15.0.0-canary.205",
    "eslint-config-prettier": "9.1.0",
    "postcss": "8.4.47",
    "postcss-load-config": "^6.0.1",
    "prettier": "3.3.3",
    "prettier-plugin-tailwindcss": "0.6.8",
    "tailwindcss": "3.4.14",
    "tsx": "4.19.1",
    "typescript": "5.6.3"
  },
  "packageManager": "pnpm@9.12.1",
  "pnpm": {
    "overrides": {
      "@types/pg": "8.11.10",
      "@types/react": "npm:types-react@rc",
      "@types/react-dom": "npm:types-react-dom@rc",
      "@neondatabase/serverless": "0.10.1",
      "cookie": "0.7.2",
      "postcss": "8.4.47",
      "ai>@ai-sdk/solid": "-",
      "ai>@ai-sdk/svelte": "-",
      "ai>@ai-sdk/vue": "-",
      "swr": "2.2.6-beta.4"
    }
  },
  "engines": {
    "node": "20.x"
  }
}
